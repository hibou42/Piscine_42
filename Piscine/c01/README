Ex07 / j ai fais un bout de code en aidant 

void    ft_rev_int_tab(int *tab, int size)
{
    int i = 0;
    int x = size - 1;
    int temp[x];

    while (i < size)
    {
        temp[i] = tab[x];
        x--;
        i++;
    }
    int h = 0;

    while (h < size)
    {
        tab[h] = temp[h];
        h++;
    }

}
int    main()
{
    int tab[20] = {1, 2, 3, 4};
    int    size;

    size = 4;
    ft_rev_int_tab(tab, size);
    printf("j avais raison\n");

    int i = 0;
    while (i < size)
    {
        printf("%d, ", tab[i]);
        i++;
    }

}